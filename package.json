{
  "name": "market-exporter",
  "version": "2.0.11",
  "description": "Market Exporter YML product export from WooCommerce.",
  "private": true,
  "scripts": {
    "watch": "webpack --watch --mode development",
    "compile": "clear && webpack --progress",
    "makepot": "npx pot-to-php languages/react.pot languages/react.php market-exporter",
    "translate": "clear && grunt translate && npm run makepot",
    "build": "grunt clean && npm run compile && grunt prepare && grunt build",
    "release": "./bin/deploy-to-svn.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/av3nger/market-exporter.git"
  },
  "author": "Anton Vanyukov",
  "license": "GPL-2.0+",
  "homepage": "https://wooya.ru",
  "dependencies": {
    "@wordpress/i18n": "^4.2.0",
    "perfect-scrollbar": "^1.5.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-select": "^4.3.1",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@wordpress/babel-plugin-makepot": "^4.2.0",
    "@wordpress/babel-preset-default": "^6.3.0",
    "@wordpress/eslint-plugin": "^9.1.0",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.15.2",
    "css-loader": "^5.2.7",
    "eslint": "^7.31.0",
    "file-loader": "^6.2.0",
    "grunt": "^1.4.1",
    "grunt-checktextdomain": "^1.0.1",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-wp-i18n": "^1.0.3",
    "load-grunt-tasks": "^5.1.0",
    "mini-css-extract-plugin": "^2.1.0",
    "node-sass": "^6.0.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@wordpress/babel-plugin-makepot",
        {
          "output": "languages/react.pot"
        }
      ]
    ]
  },
  "browserslist": [
    "ie > 9",
    "last 3 versions"
  ]
}
