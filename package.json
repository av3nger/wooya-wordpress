{
  "name": "wooya",
  "version": "1.0.0",
  "description": "WooYa integration suite.",
  "private": true,
  "scripts": {
    "watch": "webpack --watch --mode development",
    "compile": "clear && webpack -p --progress",
    "makepot": "npx pot-to-php languages/wooya.pot languages/wooya.php wooya"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/vcore_ltd/wooya-wordpress.git"
  },
  "author": "vCore",
  "license": "GPL-2.0+",
  "homepage": "https://wooya.ru",
  "dependencies": {
    "@wordpress/i18n": "^3.0.1",
    "isomorphic-fetch": "^2.2.1",
    "perfect-scrollbar": "^1.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@wordpress/babel-plugin-makepot": "^2.1.2",
    "@wordpress/babel-preset-default": "^3.0.1",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "core-js": "^2.5.7",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-command": "^0.4.1"
  },
  "babel": {
    "presets": [ "@wordpress/default" ],
    "plugins": [
      [ "@wordpress/babel-plugin-makepot", {
        "output": "languages/wooya.pot"
      } ]
    ]
  }
}
