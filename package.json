{
  "name": "wooya",
  "version": "1.0.0",
  "description": "WooYa integration suite.",
  "private": true,
  "scripts": {
    "watch": "webpack --watch --mode development",
    "compile": "clear && webpack -p --progress",
    "makepot": "npx pot-to-php languages/wooya.pot languages/wooya.php wooya"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/vcore_ltd/wooya-wordpress.git"
  },
  "author": "vCore",
  "license": "GPL-2.0+",
  "homepage": "https://wooya.ru",
  "dependencies": {
    "@wordpress/i18n": "^3.4.0",
    "isomorphic-fetch": "^2.2.1",
    "perfect-scrollbar": "^1.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-select": "^2.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@wordpress/babel-plugin-makepot": "^3.1.0",
    "@wordpress/babel-preset-default": "^4.2.0",
    "@wordpress/eslint-plugin": "^2.2.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "core-js": "^3.1.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.32.2",
    "webpack-command": "^0.4.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@wordpress/babel-plugin-makepot",
        {
          "output": "languages/wooya.pot"
        }
      ]
    ]
  }
}
