{
  "name": "market-exporter",
  "version": "2.1.0-beta.1",
  "description": "Market Exporter YML product export from WooCommerce.",
  "private": true,
  "scripts": {
    "watch": "webpack --watch --mode development",
    "compile": "clear && webpack -p --progress",
    "makepot": "npx pot-to-php languages/react.pot languages/react.php market-exporter",
    "translate": "clear && grunt translate && npm run makepot",
    "build": "grunt clean && npm run compile && grunt prepare && grunt build",
    "release": "sh bin/deploy-to-svn.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/av3nger/market-exporter.git"
  },
  "author": "Anton Vanyukov",
  "license": "GPL-2.0+",
  "homepage": "https://wooya.ru",
  "dependencies": {
    "@wordpress/i18n": "^3.7.0",
    "perfect-scrollbar": "^1.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-select": "^3.0.8",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@wordpress/babel-plugin-makepot": "^3.3.0",
    "@wordpress/babel-preset-default": "^4.7.0",
    "@wordpress/eslint-plugin": "^3.2.0",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.5.0",
    "css-loader": "^3.3.2",
    "eslint": "^6.7.2",
    "file-loader": "^5.0.2",
    "grunt": "^1.0.4",
    "grunt-checktextdomain": "^1.0.1",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^1.6.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-wp-i18n": "^1.0.3",
    "load-grunt-tasks": "^5.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "webpack": "^4.41.2",
    "webpack-command": "^0.5.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@wordpress/babel-plugin-makepot",
        {
          "output": "languages/react.pot"
        }
      ]
    ]
  },
  "browserslist": [
    "ie > 9",
    "last 3 versions"
  ]
}
